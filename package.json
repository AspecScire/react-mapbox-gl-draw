{
  "name": "react-mapbox-gl-draw",
  "version": "1.0.4",
  "description": "Draw tools for Mapbox with React: react-mapbox-gl + mapbox-gl-draw",
  "main": "lib/",
  "repository": "https://github.com/amaurymartiny/react-mapbox-gl-draw",
  "author": "Amaury Martiny",
  "license": "MIT",
  "scripts": {
    "compile": "rimraf lib/ && babel -d lib/ src/",
    "build": "npm run compile",
    "watch": "babel --watch -d lib/ src/",
    "lint": "semistandard src/**/*.js --parser babel-eslint",
    "test": "npm run lint"
  },
  "peerDependencies": {
    "@mapbox/mapbox-gl-draw": "^1.0.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-mapbox-gl": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2",
    "semistandard": "^12.0.1"
  }
}
